                  <Image url="@Pics/3.png" />
               </image>
            </ImageView>
         </children>
      </VBox>
      <VBox id="EditAccountbox" fx:id="Editacc" layoutX="183.0" layoutY="81.0" prefHeight="119.0" prefWidth="138.0">
