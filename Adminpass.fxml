               <image>
                  <Image url="@Pics/35.png" />
               </image>
            </ImageView>
         </children>
      </VBox>
      <VBox id="RemoveAccount" fx:id="Removeacc" layoutX="27.0" layoutY="244.0" prefHeight="119.0" prefWidth="150.0">
