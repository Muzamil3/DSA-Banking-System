<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@Adminpassstyle.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.banking_system.Adminpasscontroller">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Pics/internet-banking-online-payment-technology-concept.jpg" />
